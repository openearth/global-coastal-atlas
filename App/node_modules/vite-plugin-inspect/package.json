{
  "name": "vite-plugin-inspect",
  "type": "module",
  "version": "0.7.40",
  "packageManager": "pnpm@8.8.0",
  "description": "Inspect the intermediate state of Vite plugins",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/vite-plugin-inspect#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/vite-plugin-inspect.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/vite-plugin-inspect/issues"
  },
  "keywords": [
    "vite-plugin"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rimraf dist && run-s build:*",
    "build:client": "vite build src/client",
    "build:js": "unbuild",
    "dev": "nr stub && cross-env INSPECT_DEV=true vite src/client",
    "dev:client": "vite build src/client --watch",
    "stub": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "vite": "^3.1.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/utils": "^0.7.6",
    "@rollup/pluginutils": "^5.0.4",
    "debug": "^4.3.4",
    "error-stack-parser-es": "^0.1.1",
    "fs-extra": "^11.1.1",
    "open": "^9.1.0",
    "picocolors": "^1.0.0",
    "sirv": "^2.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.18",
    "@antfu/ni": "^0.21.8",
    "@iconify/json": "^2.2.122",
    "@nuxt/kit": "^3.7.4",
    "@types/codemirror": "^5.60.10",
    "@types/debug": "^4.1.9",
    "@types/diff-match-patch": "^1.0.34",
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^20.8.0",
    "@unocss/eslint-config": "^0.56.4",
    "@unocss/eslint-plugin": "^0.56.4",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/compiler-sfc": "^3.3.4",
    "@vueuse/core": "^10.4.1",
    "@vueuse/router": "^10.4.1",
    "bumpp": "^9.2.0",
    "codemirror": "^5.65.8",
    "codemirror-theme-vars": "^0.1.2",
    "comlink": "^4.4.1",
    "cross-env": "^7.0.3",
    "diff-match-patch": "^1.0.5",
    "echarts": "^5.4.3",
    "eslint": "^8.50.0",
    "esno": "^0.17.0",
    "fuse.js": "^6.6.2",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "ohash": "^1.1.3",
    "prism-theme-vars": "^0.2.4",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "splitpanes": "^3.1.5",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.56.4",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vis-data": "7.1.7",
    "vis-network": "9.1.6",
    "vite": "^4.4.9",
    "vite-dev-rpc": "^0.1.2",
    "vite-hot-client": "^0.2.2",
    "vite-plugin-pages": "^0.31.0",
    "vue": "^3.3.4",
    "vue-echarts": "^6.6.1",
    "vue-router": "^4.2.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
